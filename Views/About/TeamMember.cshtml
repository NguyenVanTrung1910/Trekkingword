@model TourWeb.Models.Account
        <!--Team Member-->
        <section class="team-single">

            <div class="floated-icon left"><img src="/images/resource/dotted-pattern-1.png" alt="" title=""></div>
            <div class="floated-icon right"><img src="/images/resource/stones-right.svg" alt="" title=""></div>
            <div class="auto-container">
                <div class="content-box">
                    <div class="row clearfix">
                        <!--Image Col-->
                        <div class="image-col col-lg-6 col-md-6 col-sm-12">
                            <div class="inner">
                                <div class="image">
                                    <div class="image"><img src="/images/@Model.ImagePath" alt="" title=""></div>
                                </div>
                            </div>
                        </div>
                        <!--Content Col-->
                        <div class="content-col col-lg-6 col-md-6 col-sm-12">
                            <div class="inner">
                                <div class="member-info">
                                    <h4 class="name">@Model.Name</h4>
                                    <div class="designation">@Model.Role</div>
                                </div>
                                <div class="member-about">
                                    <h6>About Me :</h6>
                                    <div class="text">@Model.Detail</div>
                                </div>
                                <div class="member-contact">
                                    <div class="phone"><span class="icon"><img src="/images/icons/phone-1.svg" alt="" title=""></span> Phone: <a href="tel:+123456789">@Model.PhoneNumber</a></div>
                                    <div class="phone"><span class="icon"><img src="/images/icons/email-1.svg" alt="" title=""></span> Email: <a href="@Model.Email"><span class="__cf_email__" data-cfemail="ea8f928b879a868faa9e988f818f98c4898587">[email&#160;protected]</span></a></div>
                                </div>
                                <div class="member-quote">
                                    <div class="icon"><img src="/images/icons/quotes-4.svg" alt="" title=""></div>
                                    <div class="quote">Producing Ideas Is The Main Way To Grow The Economy Life Insurance for Stay.</div>
                                </div>
                                <div class="social-links">
                                    <ul class="clearfix">
                                        <li><a href="#"><i class="fab fa-facebook-f"></i></a></li>
                                        <li><a href="#"><i class="fab fa-twitter"></i></a></li>
                                        <li><a href="#"><i class="fab fa-youtube"></i></a></li>
                                        <li><a href="#"><i class="fab fa-instagram"></i></a></li>
                                    </ul>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </section>

